<AnchorPane prefHeight="400" prefWidth="275" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoginScreenController">
   <children>
      <Button layoutX="111.0" layoutY="265.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <PasswordField fx:id="passwordTextField" layoutX="53.0" layoutY="200.0" />
      <Label layoutX="113.0" layoutY="138.0" text="Login">
         <font>
            <Font size="18.0" />
         </font></Label>
      <TextField fx:id="usernameTextField" layoutX="53.0" layoutY="165.0" />
      <Label fx:id="statusLabel" layoutX="14.0" layoutY="14.0" />
      <ImageView fx:id="userImage" fitHeight="95.0" fitWidth="79.0" layoutX="100.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../res/drawable/user.png" />
         </image>
      </ImageView>

   </children>
</AnchorPane>
